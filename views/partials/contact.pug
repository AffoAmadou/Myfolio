.contact 
    .contact__wrapper
        h1.contact__title="Let's create beautiful things together"
        .about__social
            a.about__linkedin.normal(data-animation="paragraph"  target="_blank" href=about.data.body[0].primary.linkedin)="👋🏾 LinkedIn"
            a.about__instagram.normal(data-animation="paragraph"  target="_blank" href=about.data.body[0].primary.instagram)="👋🏾 Instagram"
            a.about__mail.normal(data-animation="paragraph"  target="_blank" href="mailto:"+about.data.body[0].primary.mail)="👋🏾 Mail"
        form.contact__form(name="contact" method="POST")
            input.contact__form__input(type="text" name="name" placeholder="Name" required)
            input.contact__form__input(type="email" name="email" placeholder="Email" required)
            textarea.contact__form__textarea(name="message" placeholder="Message" required)
            button.contact__form__button(type="submit" onClick="send()")="Send"


